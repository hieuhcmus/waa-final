{
  "swagger" : "2.0",
  "info" : {
    "description" : "This is RESTFul web services definition to interact with the Airline services",
    "version" : "1.0",
    "title" : "WAA final project",
    "contact" : {
      "name" : "Van Hieu Tran",
      "email" : "vhtran@mum.edu"
    }
  },
  "host" : "localhost:8080",
  "basePath" : "/rest",
  "schemes" : [ "http", "https" ],
  "paths" : {
    "/airlines" : {
      "get" : {
        "operationId" : "getAllAirlines",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Airline"
              }
            }
          }
        }
      }
    },
    "/airlines/{id}" : {
      "get" : {
        "operationId" : "getAirline",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "type" : "integer",
          "format" : "int64"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/Airline"
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "Airline" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer",
          "format" : "int64"
        },
        "name" : {
          "type" : "string"
        }
      }
    }
  }
}